{
  "name": "react-ts-intl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf -rf dist",
    "clean:build": "yarn clean dist out build",
    "start": "yarn clean && webpack-dev-server --config webpack.config.dev.js",
    "prettier:fix:i18n": "prettier 'i18n/**.json' --write=true",
    "dev": "yarn start",
    "build": "webpack --config webpack.config.prod.js",
    "intl:extract": "formatjs extract \"src/**/*.ts*\" --out-file i18n/messages.json --format scripts/react-intl-transformer.js && yarn prettier:fix:i18n",
    "intl:json2pot": "rip json2pot 'i18n/messages.json' -o 'i18n/messages.pot'",
    "intl:poedit": "yarn intl:extract && yarn intl:json2pot",
    "intl:po-c": "rip po2json 'i18n/translations/*.po' -m 'i18n/messages.json' -o 'i18n/translation.json' && yarn prettier:fix:i18n"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "antd": "^4.6.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-intl": "^8.2.5",
    "babel-preset-react-app": "^9.1.2",
    "cors": "^2.8.5",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.8.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^1.3.0"
  },
  "engine": {
    "node": ">=12"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^2.13.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.22",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-mock-extended": "^1.0.10",
    "jest-next-dynamic": "^1.0.1",
    "json-schema-faker": "^0.5.0-rcv.29",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.11.3",
    "prettier": "^2.1.2",
    "react-intl-po": "^2.2.2",
    "resolve-ts-aliases": "^1.0.1",
    "semantic-release": "^19.0.3",
    "source-map-loader": "^1.1.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  }
}
